{
 "name": "daniel-blake-design",
 "version": "1.0.0",
 "description": "Daniel Blake is a front end web designer living in Portland, OR.",
 "homepage": "https://danielblake.design",
 "repository": "https://github.com/dmbdesignpdx/danielblakedesign.git",
 "author": "Daniel Blake <daniel@danielblake.design>",
 "license": "UNLICENSED",
 "private": true,
 "scripts": {
  "clean:webpack": "rm -r static/dist",
  "clean:hugo": "rm -r public",
  "prehugo": "yarn clean:hugo",
  "hugo": "hugo",
  "publish": "env HUGO_ENV=production hugo --minify",
  "precompile": "yarn clean:webpack",
  "compile": "webpack --mode development",
  "webpack:dev": "webpack --mode development --watch",
  "hugo:dev": "hugo serve -p 3000",
  "dev": "npm-run-all -s clean:hugo clean:webpack -p webpack:dev hugo:dev",
  "prebuild": "yarn clean:webpack",
  "build": "webpack --mode production",
  "lint": "eslint src/*.js"
 },
 "devDependencies": {
  "@babel/core": "^7.0.0",
  "@babel/polyfill": "^7.0.0",
  "@babel/preset-env": "^7.0.0",
  "babel-loader": "^8.0.2",
  "browser-sync": "^2.23.6",
  "eslint": "^5.7.0",
  "jest": "^23.6.0",
  "npm-run-all": "^4.1.5",
  "turntable-kit": "^0.7.0",
  "webpack": "^4.17.2",
  "webpack-cli": "^3.1.0"
 }
}
