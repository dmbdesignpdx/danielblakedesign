{
 "name": "daniel-blake-design",
 "version": "1.0.0",
 "description": "Daniel Blake is a front end web designer living in Portland, OR.",
 "homepage": "https://danielblake.design",
 "repository": "https://github.com/dmbdesignpdx/danielblakedesign.git",
 "author": "Daniel Blake <daniel@danielblake.design>",
 "license": "UNLICENSED",
 "private": true,
 "scripts": {
  "clean:js": "rm static/dist/main.min.js",
  "clean:hugo": "hugo --cleanDestinationDir",
  "hugo:min": "env HUGO_ENV=production hugo --minify",
  "serve": "hugo serve -p 3000",
  "serve:nolr": "hugo serve -p 3000 --disableLiveReload",
  "serve:min": "env HUGO_ENV=production hugo serve -p 3000 --disableLiveReload --minify",
  "prewebpack": "yarn clean:js",
  "webpack": "webpack --mode development",
  "prewebpack:watch": "webpack --mode development --watch",
  "webpack:watch": "webpack --mode development --watch",
  "dev": "npm-run-all -p webpack:watch serve",
  "prebuild": "yarn clean:js",
  "build": "webpack --mode production",
  "preview": "npm-run-all -s build serve:min",
  "lint": "eslint src/*.js"
 },
 "devDependencies": {
  "@babel/core": "^7.0.0",
  "@babel/polyfill": "^7.0.0",
  "@babel/preset-env": "^7.0.0",
  "babel-loader": "^8.0.2",
  "css-mqpacker": "^7.0.0",
  "eslint": "^5.7.0",
  "npm-run-all": "^4.1.5",
  "postcss": "^7.0.14",
  "postcss-clean": "^1.1.0",
  "postcss-merge-rules": "^4.0.3",
  "postcss-merge-selectors": "^0.0.6",
  "turntable-kit": "^0.7.0",
  "webpack": "^4.17.2",
  "webpack-cli": "^3.1.0"
 }
}
