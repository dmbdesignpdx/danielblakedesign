@-ms-viewport{width:device-width}*{box-sizing:border-box}html{font:normal 18px system-ui, sans-serif;background:#f1f1f4}body{-moz-osx-font-smoothing:grayscale;-webkit-font-smoothing:antialiased;-webkit-text-size-adjust:none;text-size-adjust:none}body,h1{margin:0}main{inline-size:100vw;block-size:100vh;display:flex;flex-direction:column;justify-content:center;align-items:center}h1{right:.02em;inset-inline-end:.02em;position:relative;font-size:30vw;color:#17161d;line-height:1;animation:intro 2s 0.5s both}div{text-align:center;animation:afterIntro 1s 2.6s both}@keyframes intro{0%{transform:translateY(-66%);opacity:0}100%{transform:translateY(0);opacity:1}}@keyframes afterIntro{0%{opacity:0}100%{opacity:1}}p{margin-block-start:1.6em;margin-inline-end:0;margin-block-end:2em;margin-inline-start:0;color:#5a5671}a{block-size:50px;padding-block-start:0;padding-inline-end:1.5em;padding-block-end:0;padding-inline-start:1.5em;margin-block-end:1em;display:inline-block;font-size:0.9em;font-weight:bold;line-height:50px;letter-spacing:0.02em;text-decoration:none;text-align:center;color:#f7f6f8;border-radius:3px;background:#dd2c53;box-shadow:0 1px 2px rgba(0,0,30,.35);-webkit-tap-highlight-color:transparent;cursor:pointer}@supports (-moz-binding: none){a{line-height:52px}}@supports (-webkit-marquee: revert){a{line-height:48px}}a:focus,a:hover{background:#e14265;box-shadow:0 2px 5px rgba(0,0,30,.35)}a:active{box-shadow:0 1px 1px rgba(0,0,30,.35)}@media (min-width: 520px){html{font-size:20px}}
